  <body class="nav-md">
    <div class="container body">
      <div class="main_container">
        <div class="col-md-3 left_col">
          <div class="left_col scroll-view">
            <div class="navbar-inner" style="border: 0;">
              <a href="/principal/home" class="site_title"><i class="fa fa-line-chart" aria-hidden="true"></i> <span>Renaware</span></a>
            </div>

            <div class="clearfix"></div>

            <br />

            <!-- sidebar menu -->
            <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
              <div class="menu_section">
                
                <ul class="nav side-menu">
                  <li><a><i class="fa fa-edit"></i> Ventas <span class="fa fa-chevron-down"></span></a>
                    <ul class="nav child_menu">
                      <li><a href="/principal/ventas">Ventas</a></li>
                      
                      <li><a href="/clientes">Clientes</a></li>
                    </ul>
                  </li>
                  <li><a><i class="fa fa-bar-chart-o"></i> Dashboard <span class="fa fa-chevron-down"></span></a>
                    <ul class="nav child_menu">
                      <li><a href="/principal/home">Estadistica</a></li>
                    </ul>
                  </li>
                  
                  <li><a><i class="fa fa-truck" aria-hidden="true"></i> Env√≠os <span class="fa fa-chevron-down"></span></a>
                    <ul class="nav child_menu">
                      <li><a href="/envio/envios">Seguimiento</a></li>
                    </ul>
                  </li>
