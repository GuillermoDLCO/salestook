<%= javascript_include_tag 'vendors/moment/moment.min.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'vendors/bootstrap-daterangepicker/daterangepicker.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'build/js/custom.min.js', 'data-turbolinks-track': 'reload' %>
  </body>
</html>
